def is_prime(number):
    if number<=1:
        return False
    for i in range(2,number):
        if(number %i == 0):
            return False
    return True 

def main():
    number = int(input("enter  number:"))
    result = is_prime(number)
    if result:
        print(f"{number} is a prime number.")
    else:
        print(f"{number} is not a prime number.")
 
if __name__ == "__main__":
 main()
